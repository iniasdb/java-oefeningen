<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="267.0" prefWidth="297.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="297.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="open" mnemonicParsing="false" text="Open">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="O" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem fx:id="save" mnemonicParsing="false" text="Save">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem fx:id="close" mnemonicParsing="false" text="Close">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="C" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label fx:id="lln" layoutX="236.0" layoutY="47.0" prefHeight="17.0" prefWidth="28.0" />
      <Label layoutX="14.0" layoutY="90.0" text="Vak" />
      <Label fx:id="vaknaam" layoutX="65.0" layoutY="90.0" />
      <Label layoutX="15.0" layoutY="116.0" text="Punten" />
      <Label fx:id="punten" layoutX="65.0" layoutY="116.0" />
      <Button layoutX="198.0" layoutY="78.0" mnemonicParsing="false" onAction="#correct" text="Correct" />
      <Button layoutX="200.0" layoutY="112.0" mnemonicParsing="false" onAction="#nietCorrect" text="Niet Correct" />
      <Label layoutX="10.0" layoutY="164.0" text="Punt aanpassen" />
      <TextField fx:id="nieuwPunt" layoutX="10.0" layoutY="181.0" prefHeight="25.0" prefWidth="81.0" />
      <Button layoutX="200.0" layoutY="181.0" mnemonicParsing="false" onAction="#aanpassen" text="Aanpassen" />
   </children>
</AnchorPane>
